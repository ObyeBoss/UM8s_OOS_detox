<?xml version="1.0" encoding="UTF-8"?>

<filter-conf>
    <version>20240826</version>
    <toggle>1</toggle>
    <features>
        <feature name="BindCore" enable="0">
            <!-- big core index, start from 0 -->
            <bigcore_id>4</bigcore_id>
            <bigcore_id>5</bigcore_id>
            <bigcore_id>6</bigcore_id>
            <bigcore_id>7</bigcore_id>
        </feature>

        <feature name="LocalHdr" enable="1">
            <support10bit>enable</support10bit>
        </feature>

        <feature name="AuroraWallpaper" enable="1">
            <support10bit>enable</support10bit>
        </feature>

        <feature name="GameEdr" enable="1">
        </feature>

        <feature name="GpuSwFence" enable="1">
        </feature>

        <feature name="HdrVision" enable="1">
            <timeout_s>60</timeout_s>
            <scale>6</scale>
            <timerEnable>1</timerEnable>
            <application name="com.netflix.mediaclient"></application>
            <application name="com.amazon.avod.thirdpartyclient"></application>
            <application name="com.google.android.youtube"></application>
            <application name="app.revanced.android.youtube"></application>
        </feature>

        <feature name="OplusDolbyVision" enable="1">
            <timeout_s>60</timeout_s>
            <scale>6</scale>
            <timerEnable>1</timerEnable>
            <needDimming>1</needDimming>
            <fullScreenOnly>0</fullScreenOnly>
            <application name="com.netflix.mediaclient"/>
        </feature>

        <feature name="curtain" enable="1">
            <application name="com.instagram.android">
                <scene type="FuzzyLayer">
                    <layer>TransparentModalActivity</layer>
                    <action>
                        <mode>Normal</mode>
                    </action>
                </scene>
                <scene type="FuzzyLayer">
                    <layer>MainActivity</layer>
                    <action>
                        <mode>Normal</mode>
                    </action>
                </scene>
                <scene type="FuzzyLayer">
                    <layer>ModalActivity</layer>
                    <action>
                        <mode>Normal</mode>
                    </action>
                </scene>
            </application>
            <application name="com.google.android.youtube">
                <scene type="FuzzyLayer">
                    <layer>WatchWhileActivity</layer>
                    <action>
                        <mode>SurfaceView</mode>
                    </action>
                </scene>
                <scene type="FuzzyLayer">
                    <layer>InternalMainActivity</layer>
                    <action>
                        <mode>SurfaceView</mode>
                    </action>
                </scene>
                <scene type="FuzzyLayer">
                    <layer>HomeActivity</layer>
                    <action>
                        <mode>SurfaceView</mode>
                    </action>
                </scene>
            </application>
            <application name="app.revanced.android.youtube">
                <scene type="FuzzyLayer">
                    <layer>WatchWhileActivity</layer>
                    <action>
                        <mode>SurfaceView</mode>
                    </action>
                </scene>
                <scene type="FuzzyLayer">
                    <layer>InternalMainActivity</layer>
                    <action>
                        <mode>SurfaceView</mode>
                    </action>
                </scene>
                <scene type="FuzzyLayer">
                    <layer>HomeActivity</layer>
                    <action>
                        <mode>SurfaceView</mode>
                    </action>
                </scene>
            </application>
        </feature>

        <feature name="Sharpness" enable="1">
            <application name="com.zhiliaoapp.musically">
                <scene type="Application">
                    <action>
                        <requestResolution>1</requestResolution>
                        <featureId>5</featureId>
                        <featureValuesOn>2</featureValuesOn>
                        <featureValuesOff>1</featureValuesOff>
                    </action>
                </scene>
            </application>

            <application name="com.mxtech.videoplayer.ad">
                <scene type="Application">
                    <action>
                        <requestResolution>1</requestResolution>
                        <featureId>5</featureId>
                        <featureValuesOn>2</featureValuesOn>
                        <featureValuesOff>1</featureValuesOff>
                    </action>
