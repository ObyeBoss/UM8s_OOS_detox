<?xml version="1.0" encoding="utf-8"?>
<config name="scene_event">
    <filter-conf>
        <version>20250517</version>
        <filter-name>sys_osense_scene_event_config</filter-name>
    </filter-conf>
    <!-- 应用生命周期及UI保证线程 -->
    <event id="10" name="APP_MOVE_TO_TOP" maxWorkDurationMs="-1" source_caller="android">
        <key-thread name="UIThread" owner="tp" sched_policy="ux" priority="MEDIUM_HEAVY" />
        <key-thread name="RenderThread" owner="tp" sched_policy="ux" priority="MEDIUM_HEAVY" />
        <key-thread name="HwUiTask" owner="tp" sched_policy="ux" priority="HIGH_LIGHT" />
        <key-thread name="GLThread" owner="tp" sched_policy="ux" priority="MEDIUM_HEAVY" />
        <key-thread name="UIThread" owner="fg1" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="RenderThread" owner="fg1" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="HwUiTask" owner="fg1" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="GLThread" owner="fg1" sched_policy="ux" priority="MEDIUM_LIGHT" />
    </event>
    <!--标识 system_server 启动完成且系统服务加载完毕-->
    <event id="20" name="SYSTEM_BOOT_COMPLETE" maxWorkDurationMs="-1" source_caller="android">
        <vip-binder>default</vip-binder>
    </event>
    <event id="50" name="APP_COLD_LAUNCH" maxWorkDurationMs="1000" source_caller="android">
        <key-thread name="UIThread" owner="tp" sched_policy="ux" priority="MEDIUM_LIGHT" />
    </event>

    <event id="101" name="DISPLAY_DPS" source_caller="android">
        <key-thread name="PhotonicModulator" owner="android" sched_policy="ux" priority="MEDIUM_HEAVY" />
    </event>

    <!-- FaceHal 线程 begin -->
    <event id="500" name="FACE_UNLOCK_BOOST" maxWorkDurationMs="-1" source_caller="face_hal">
        <key-thread name="FaceManager" owner="face_hal" sched_policy="ux" priority="MEDIUM_HEAVY" />
    </event>
    <!-- FaceHal 线程 end -->

    <!-- systemserver 关键线程调度场景begin  -->
    <event id="1001" name="PENDING_MOTION_FOR_SYSTEMUI" maxWorkDurationMs="500" source_caller="android">
        <key-thread name="UIThread" owner="systemui" sched_policy="ux" priority="MEDIUM_LIGHT" />
    </event>

    <event id="1004" name="SYSTEM_FOLDSCREEN_SPEED" maxWorkDurationMs="1000" source_caller="android">
        <key-thread name="android.ui" owner="android" sched_policy="ux" priority="LIGHT" />
        <key-thread name="android.display" owner="android" sched_policy="ux" priority="LIGHT" />
        <key-thread name="PowerManagerService" owner="android" sched_policy="ux" priority="LIGHT" />
        <key-thread name="PhotonicModulator" owner="android" sched_policy="ux" priority="LIGHT" />
        <key-thread name="oplus.io" owner="android" sched_policy="ux" priority="LIGHT" />
        <key-thread name="UIThread" owner="launcher" sched_policy="ux" priority="LIGHT" />
        <!-- SystemUI threads begin -->
        <key-thread name="UIThread" owner="systemui" sched_policy="ux" priority="LIGHT" />
        <key-thread name="RenderThread" owner="systemui" sched_policy="ux" priority="MEDIUM_HEAVY" />
        <key-thread name="HwUiTask" owner="systemui" sched_policy="ux" priority="LIGHT" />
        <key-thread name="GLThread" owner="systemui" sched_policy="ux" priority="LIGHT" />
        <key-thread name="ImageWallpaper" owner="systemui" sched_policy="ux" priority="LIGHT" />
        <key-thread name="SysUiLng" owner="systemui" sched_policy="ux" priority="LIGHT" />
        <!-- SystemUI threads end -->
    </event>

    <event id="1006" name="SYSTEM_FAST_FREEZE" maxWorkDurationMs="500" source_caller="android">
        <key-thread name="HansMainHandler" owner="android" sched_policy="ux" priority="LIGHT" />
    </event>
    <event id="1007" name="SYSTEM_OSENSE_KILL" maxWorkDurationMs="500" source_caller="android">
        <key-thread name="osense.kill" owner="android" sched_policy="ux" priority="LIGHT" />
    </event>
    <event id="1008" name="DISPLAY_RAMP_ANIMATOR" maxWorkDurationMs="-1" source_caller="android">
        <key-thread name="PowerManagerService" owner="android" sched_policy="ux" priority="MEDIUM_LIGHT" />
    </event>
    <event id="1009" name="SCENE_EVENT_GAME_TO_TOP_DELAY_UX" maxWorkDurationMs="-1" source_caller="android">
        <key-thread name="UIThread" owner="tp" sched_policy="ux" priority="MEDIUM_HEAVY" />
        <key-thread name="RenderThread" owner="tp" sched_policy="ux" priority="MEDIUM_HEAVY" />
    </event>
    <event id="1010" name="SCENE_EVENT_ACCESSIBILITY_UX" maxWorkDurationMs="-1" source_caller="android">
        <key-thread name="system_server" owner="android" sched_policy="ux" priority="LIGHT" />
    </event>
    <event id="1011" name="SYSTEM_ATHENA_KILL" maxWorkDurationMs="5000" source_caller="system_app">
        <key-thread name="athena_killer" owner="android" sched_policy="ux" priority="MEDIUM_LIGHT" />
    </event>
    <event id="1012" name="SYSTEM_OOM_ADJUSTER" maxWorkDurationMs="-1" source_caller="android">
        <key-thread name="OomAdjuster" owner="android" sched_policy="ux" priority="MEDIUM_LIGHT" />
    </event>
    <event id="1013" name="SCREEN_STATE_TURN_ON" maxWorkDurationMs="2000" source_caller="android">
        <key-thread name="PowerManagerService" owner="android" sched_policy="ux" priority="LISTPICK_TASK" />
    </event>
    <event id="1014" name="SCENE_EVENT_IMMS_UX" maxWorkDurationMs="5000" source_caller="android">
        <key-thread name="android.imms" owner="android" priority="HIGH_HEAVY" sched_policy="ux" />
    </event>
    <event id="1015" name="START_TOP_PROCESS" maxWorkDurationMs="1000" source_caller="android">
        <key-thread name="AppDeath #1" owner="android" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="AppDeath #2" owner="android" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="oplus.appStartPerf" owner="android" sched_policy="ux" priority="MEDIUM_LIGHT" />
    </event>
    <!-- systemserver 关键线程调度场景end  -->

    <!-- Launcher Key thread scheduling scenarios begin  -->
    <event id="2000" name="LAUNCHER_STATIC_UX" source_caller="android">
        <key-thread name="UIThread" owner="launcher" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="RenderThread" owner="launcher" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="HwUiTask" owner="launcher" sched_policy="ux" priority="LIGHT" />
    </event>
    <event id="2001" name="LAUNCHER_ASSIST_SCREEN_SCROLL" maxWorkDurationMs="60000"
        source_caller="launcher|assistscreen">
        <key-thread name="UIThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="RenderThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="HwUiTask" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="GLThread" owner="launcher" sched_policy="ux" priority="LIGHT" />
        <key-thread name="launcher-loader" owner="launcher" sched_policy="ux" priority="MEDIUM_HEAVY" />
        <key-thread name="UIThread" owner="assistscreen" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="RenderThread" owner="assistscreen" sched_policy="ux"
            priority="ANIMATOR" />
        <key-thread name="HwUiTask" owner="assistscreen" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="GLThread" owner="assistscreen" sched_policy="ux" priority="LIGHT" />
        <key-thread name="android.anim" owner="android" sched_policy="ux" priority="ANIMATOR" />
        <vip-binder>assisitscreen-slide-to-launcher</vip-binder>
    </event>
    <event id="2002" name="LAUNCHER_START_APP" maxWorkDurationMs="2000" source_caller="launcher">
        <key-thread name="UIThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="RenderThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="HwUiTask" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="GLThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="android.anim" owner="android" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="ll.splashscreen" owner="systemui" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="ll.splashworker" owner="systemui" sched_policy="ux" priority="ANIMATOR" />
        <vip-binder>launcher-app-start</vip-binder>
    </event>
    <event id="2003" name="LAUNCHER_HIDE_KEYBOARD" maxWorkDurationMs="1000" source_caller="launcher">
        <key-thread name="UiThreadHelper" owner="launcher" sched_policy="ux" priority="LIGHT" />
    </event>
    <event id="2004" name="LAUNCHER_CONNECT_ASSIST_SCREEN" maxWorkDurationMs="1000" source_caller="launcher">
        <key-thread name="UiThreadHelper" owner="launcher" sched_policy="ux" priority="LIGHT" />
    </event>
    <event id="2005" name="LAUNCHER_BRACKET_SPACE_UX" maxWorkDurationMs="1000" source_caller="launcher">
        <key-thread name="TaskViewTransactionHelper" owner="launcher" sched_policy="ux" priority="LIGHT" />
    </event>
    <event id="2006" name="LAUNCHER_FOLDER_ANIMATION" maxWorkDurationMs="2000"
        source_caller="launcher">
        <key-thread name="UIThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="RenderThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="HwUiTask" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="GLThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
    </event>
    <event id="2007" name="LAUNCHER_RECENTS" maxWorkDurationMs="2000"
        source_caller="launcher">
        <key-thread name="UIThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="RenderThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="HwUiTask" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="GLThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="UrgentTransactionHelper" owner="launcher" sched_policy="ux" priority="LIGHT" />
        <!-- <key-thread name="UIThread" owner="systemui" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="RenderThread" owner="systemui" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="HwUiTask" owner="systemui" sched_policy="ux" priority="LIGHT" />  -->
    </event>
    <event id="2008" name="LAUNCHER_TASKBAR_SUBSCRIBE_PRESS" maxWorkDurationMs="2000"
        source_caller="launcher">
        <key-thread name="UIThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="RenderThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="HwUiTask" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="GLThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="UrgentTransactionHelper" owner="launcher" sched_policy="ux" priority="LIGHT" />
        <key-thread name="UiThreadHelper" owner="launcher" sched_policy="ux" priority="LIGHT" />
    </event>
    <event id="2009" name="LAUNCHER_DRAG" maxWorkDurationMs="6000"
        source_caller="launcher">
        <key-thread name="UIThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="RenderThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="HwUiTask" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="GLThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
    </event>
    <event id="2010" name="LAUNCHER_FOLDER_ICON_FALLEN" maxWorkDurationMs="2000"
        source_caller="launcher">
        <key-thread name="UIThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="RenderThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="HwUiTask" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="GLThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
    </event>
    <event id="2011" name="LAUNCHER_GESTURE_WINDOW_ANIMATION" maxWorkDurationMs="2000"
        source_caller="launcher">
        <key-thread name="UIThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="RenderThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="HwUiTask" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="GLThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <vip-binder>imms-focus-change</vip-binder>
    </event>
    <event id="2012" name="LAUNCHER_SUPER_POWER_MODE_EXIT" maxWorkDurationMs="2000" source_caller="launcher">
        <key-thread name="UIThread" owner="launcher" sched_policy="ux" priority="LIGHT" />
        <!-- SystemUI threads begin -->
        <key-thread name="UIThread" owner="systemui" sched_policy="ux" priority="LIGHT" />
        <key-thread name="RenderThread" owner="systemui" sched_policy="ux" priority="LIGHT" />
        <key-thread name="HwUiTask" owner="systemui" sched_policy="ux" priority="LIGHT" />
        <key-thread name="GLThread" owner="systemui" sched_policy="ux" priority="LIGHT" />
        <key-thread name="ImageWallpaper" owner="systemui" sched_policy="ux" priority="LIGHT" />
        <!-- SystemUI threads end -->
    </event>
    <event id="2013" name="LAUNCHER_ICON_PRESS" maxWorkDurationMs="2000"
        source_caller="launcher">
        <key-thread name="UIThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="RenderThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="HwUiTask" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="GLThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
    </event>
    <event id="2014" name="LAUNCHER_FINISH_CONTROLLER" maxWorkDurationMs="1000">
        <key-thread name="UrgentTransactionHelper" owner="launcher" sched_policy="ux" priority="LIGHT" />
    </event>
    <event id="2015" name="LAUNCHER_WORKSPACE_SCROLL" maxWorkDurationMs="2000"
        source_caller="launcher">
        <key-thread name="UIThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="RenderThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="HwUiTask" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="GLThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
    </event>
    <event id="2016" name="LAUNCHER_STATIC_LAUNCHER_ANIM" maxWorkDurationMs="-1" source_caller="launcher">
       <key-thread name="launcher.anim" owner="launcher" sched_policy="ux" priority="HIGH_ANIMATOR" />
    </event>
    <event id="2017" name="LAUNCHER_STATIC_ONLINE_UX" maxWorkDurationMs="-1" source_caller="launcher">
       <key-thread name="onlineUXThread" owner="launcher" sched_policy="ux" priority="HIGH_ANIMATOR" />
    </event>
    <event id="2018" name="LAUNCHER_UPDATE_TOUCH_REGION" maxWorkDurationMs="1000" source_caller="launcher">
       <key-thread name="UrgentTransactionHelper" owner="launcher" sched_policy="ux" priority="MEDIUM_LIGHT" />
    </event>
    <event id="2019" name="LAUNCHER_START_WALLPAPER_ANIMATION" maxWorkDurationMs="1000" source_caller="launcher">
       <key-thread name="WallpaperTransactionHelper" owner="launcher" sched_policy="ux" priority="MEDIUM_LIGHT" />
    </event>
    <event id="2020" name="LAUNCHER_GET_TASKS" maxWorkDurationMs="1000" source_caller="launcher">
       <key-thread name="RecentTasksList" owner="launcher" sched_policy="ux" priority="MEDIUM_LIGHT" />
    </event>
    <event name="LAUNCHER_LOAD_LAYOUT" id="2021" maxWorkDurationMs="60000" source_caller="launcher">
        <key-thread name="UIThread" owner="launcher" priority="ANIMATOR" sched_policy="ux" />
        <key-thread name="RenderThread" owner="launcher" priority="ANIMATOR" sched_policy="ux" />
        <key-thread name="HwUiTask" owner="launcher" priority="ANIMATOR" sched_policy="ux" />
        <key-thread name="GLThread" owner="launcher" priority="LIGHT" sched_policy="ux" />
        <key-thread name="launcher-loader" owner="launcher" priority="MEDIUM_HEAVY" sched_policy="ux" />
    </event>
    <event id="2022" name="LAUNCHER_STATIC_LIFECYCLE_HELPER" maxWorkDurationMs="-1" source_caller="launcher">
       <key-thread name="oplus_lifecycle_helper" owner="launcher" sched_policy="ux" priority="MEDIUM_LIGHT" />
    </event>
    <event id="2023" name="LAUNCHER_TASKICON_CACHE_POST" maxWorkDurationMs="2000"
        source_caller="launcher">
        <key-thread name="TaskIconCache" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="TaskThumbnailCache" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
    </event>
    <event id="2024" name="LAUNCHER_BROWSER_SCROLL" maxWorkDurationMs="60000"
        source_caller="launcher">
        <key-thread name="UIThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="RenderThread" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="HwUiTask" owner="launcher" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="GLThread" owner="launcher" sched_policy="ux" priority="LIGHT" />
        <key-thread name="launcher-loader" owner="launcher" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="UIThread" owner="com.heytap.browser:slide" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="RenderThread" owner="com.heytap.browser:slide" sched_policy="ux"
            priority="ANIMATOR" />
        <key-thread name="HwUiTask" owner="com.heytap.browser:slide" sched_policy="ux" priority="ANIMATOR" />
        <key-thread name="GLThread" owner="com.heytap.browser:slide" sched_policy="ux" priority="LIGHT" />
    </event>
    <!-- Launcher 关键线程调度场景end  -->

    <!-- SystemUI 关键线程调度场景begin  -->
    <!-- SystemUI状态栏下拉，保障从接收到event事件到systemui变为top app阶段  -->
    <event id="3000" name="SYSTEMUI_NOTIFICATION_SHADE_EXPAND" maxWorkDurationMs="1000"
        source_caller="systemui">
        <key-thread name="UIThread" owner="systemui" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="RenderThread" owner="systemui" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="HwUiTask" owner="systemui" sched_policy="ux" priority="LIGHT" />
        <key-thread name="GaussianBlur" owner="systemui" sched_policy="ux" priority="MEDIUM_LIGHT" />
    </event>
    <!-- 侧滑场景，收到侧滑事件到侧滑界面显示出来 -->
    <event id="3001" name="SYSTEMUI_EDGE_SWIPE_BACK_UI" maxWorkDurationMs="15000" source_caller="systemui">
        <!-- set to be MEDIUM_HEAVY for rendering animation -->
        <key-thread name="GestureUI" owner="systemui" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="RenderThread" owner="systemui" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="HwUiTask" owner="systemui" sched_policy="ux" priority="LIGHT" />
    </event>
    <!-- 侧滑场景，侧滑手势松手后退出 -->
    <event id="3002" name="SYSTEMUI_EDGE_SWIPE_BACK_EXIT" maxWorkDurationMs="400"
        source_caller="systemui">
        <!-- set to be MEDIUM_HEAVY for rendering animation -->
        <key-thread name="GestureUI" owner="systemui" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <!-- for related animation like navigation bar and status bar -->
        <key-thread name="UIThread" owner="systemui" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="RenderThread" owner="systemui" sched_policy="ux" priority="MEDIUM_LIGHT" />
    </event>
    <!-- 音量弹框 -->
    <event id="3003" name="SYSTEMUI_VOLUME_DIALOG_SHOWING" maxWorkDurationMs="5000" source_caller="systemui">
        <key-thread name="UIThread" owner="systemui" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="RenderThread" owner="systemui" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="HwUiTask" owner="systemui" sched_policy="ux" priority="LIGHT" />
        <key-thread name="VolumeDialogControllerImpl" owner="systemui" sched_policy="ux" priority="LIGHT" />
    </event>
    <!-- 显示状态栏隐私图标 -->
    <event id="3004" name="SYSTEMUI_PRIVACY_UI_SHOWING" maxWorkDurationMs="-1" source_caller="systemui">
        <key-thread name="UIThread" owner="systemui" sched_policy="ux" priority="MEDIUM_LIGHT" />
    </event>
    <!-- 状态栏显示流体云胶囊 -->
    <event id="3006" name="SYSTEMUI_SEEDLING_CAPSULE_SHOWING" maxWorkDurationMs="-1" source_caller="systemui">
        <key-thread name="UIThread" owner="systemui" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="RenderThread" owner="systemui" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="HwUiTask" owner="systemui" sched_policy="ux" priority="LIGHT" />
    </event>
    <!-- 状态栏流体云胶囊单击跳转到应用 -->
    <event id="3007" name="SYSTEMUI_SEEDLING_CAPSULE_JUMP" maxWorkDurationMs="1000" source_caller="systemui">
        <key-thread name="UIThread" owner="systemui" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="RenderThread" owner="systemui" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="HwUiTask" owner="systemui" sched_policy="ux" priority="LIGHT" />
    </event>
    <!-- 暗色模式切换 -->
    <event id="3008" name="UI_MODE_CHANGED" maxWorkDurationMs="2000" source_caller="systemui">
        <key-thread name="UIThread" owner="systemui" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="RenderThread" owner="systemui" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="HwUiTask" owner="systemui" sched_policy="ux" priority="LIGHT" />
    </event>
    <!-- 状态栏添加系统图标场景 -->
    <event id="3020" name="SYSTEMUI_SYSTEM_STATUS_ICON_ADDING" maxWorkDurationMs="2000" source_caller="systemui">
        <key-thread name="UIThread" owner="systemui" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="RenderThread" owner="systemui" sched_policy="ux" priority="MEDIUM_LIGHT" />
        <key-thread name="HwUiTask" owner="systemui" sched_policy="ux" priority="LIGHT" />
    </event>
    <!-- 进入锁屏场景，为指纹解锁线程设置ux，解锁完成后，场景结束 -->
    <event id="3100" name="SYSTEMUI_SCREEN_LOCKED" maxWorkDurationMs="-1" source_caller="systemui">
        <key-thread name="OnScreenFingerprint OpticalUIUpdateThread" owner="systemui" sched_policy="ux" priority="LIGHT" />
    </event>
    <!-- 解锁成功后的退场场景 -->
    <event id="3101" name="SYSTEMUI_KEYGUARD_GOING_AWAY" maxWorkDurationMs="2000" source_caller="systemui">
        <key-thread name="KeyguardWorkerThread" owner="systemui" sched_policy="ux" priority="MEDIUM_LIGHT" />
    </event>
    <!-- 锁屏亮灭屏壁纸/VFX动画 -->
    <event id="3102" name="SYSTEMUI_KEYGUARD_ANIM_RUNNING" maxWorkDurationMs="1000" source_caller="systemui">
